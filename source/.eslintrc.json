{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "mocha": true,
        "browser": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018
    },
    "plugins": [
        "react",
        "mocha"
    ],
    "rules": {
        "mocha/no-exclusive-tests": "error",
        "MY CUSTOM ELINT OVERRIDES": "",
        "indent": [2, "tab", { "SwitchCase": 1, "VariableDeclarator": 1 }],
        "quotes": ["error", "double"],
        "max-len": [2, 180, 4, {"ignoreUrls": true}],
        "no-tabs": 0,
        "RENlinebreak-style": 0,
        "no-multiple-empty-lines": 0,
        "prefer-template": 0,
        "no-constant-condition": 0,
        "no-param-reassign": 0,
        "vars-on-top": 0,
        "no-var": 0,
        "no-throw-literal": 0,
        "padded-blocks": 0,
        "class-methods-use-this": 0,
        "no-plusplus": 0,
        "strict": 0,
        "no-use-before-define": 0,
        "prefer-destructuring": 0,
        "one-var-declaration-per-line": 0,
        "one-var": 0,
        "object-property-newline": 0,
        "prefer-arrow-callback": 0,
        "global-require": 0,
        "no-unused-vars": 0,
        "no-underscore-dangle": 0,
        "new-cap": 0,
        "no-lonely-if": 0,
        "import/no-dynamic-require": 0,
        "no-await-in-loop": 0,
        "no-return-await": 0,
        "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
        "guard-for-in": 0,
        "no-loop-func": 0,
        "no-alert": 0
    }
}