{
  "name": "approom",
  "version": "1.0.0",
  "description": "Server backend to facilitate multi-user communication and synchronization using a room metaphor",
  "main": "watcher.js",
  "scripts": {
    "test": "cls && mocha",
    "testg": "cls && mocha --grep ",
    "test2file": "mocha > testoutput.txt 2>&1",
    "ctest2file": "mocha --grep client > testoutput.txt 2>&1",
    "mongod": "C:\\Programs\\MongoDB\\Server\\4.0\\bin\\mongod.exe -f C:\\Programs\\MongoDB\\Server\\4.0\\bin\\mongod.cfg",
    "help": "cls && node approom.js --help",
    "runserver": "cls && node approom.js runserver",
    "runtestserver": "cls && node approom.js runserver --configset testing",
    "runprodserver": "cls && cross-env NODE_ENV=production node approom.js runserver",
    "profileserver": "cls && node approom.js runserver --config profile",
    "dbsetup": "cls && node approom.js dbsetup",
    "eslint": "cls && eslint .",
    "eslintfix": "cls && eslint . --fix",
    "npmcheck": "cls && npm audit || npm-check --skip-unused || exit 0",
    "npmupdate": "cls && npm update --no-optional",
    "makedoc": "cls && jsdoc --private --readme ../README.md --configure jsdoc.json && copyfiles -u 3 ../docs/jsdoc/**/* static/jsdoc",
    "deletelogs": "cls && del-cli logs/*.log"
  },
  "author": "jesse reichler / mouser (mouser@donationcoder.com)",
  "license": "UNLICENSED",
  "repository": "dcmouser/approom",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "mocha": "^7.2.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "debug": "^4.1.1",
    "del-cli": "^3.0.1",
    "docdash": "^1.2.0",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "gravatar": "^1.8.0",
    "handlebars": "^4.7.6",
    "hbs": "^4.1.1",
    "http-errors": "^1.7.3",
    "humanize-duration": "^3.23.1",
    "jquery": "^3.5.1",
    "js-yaml": "^3.14.0",
    "jsonwebtoken": "^9.0.0",
    "merge-deep": "^3.0.2",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.20",
    "morgan": "^1.10.0",
    "nconf": "^0.10.0",
    "nodemailer": "^6.4.10",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "popper.js": "^1.16.1",
    "rate-limiter-flexible": "^2.1.7",
    "redis": "^3.0.2",
    "role-acl": "^4.5.4",
    "serve-favicon": "^2.5.0",
    "v8-profiler-next": "^1.3.0",
    "validator": "^13.1.1",
    "winston": "^3.3.2",
    "yargs": "^15.3.1"
  }
}
