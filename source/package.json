{
  "name": "approom",
  "version": "1.0.0",
  "description": "Server backend to facilitate multi-user communication and synchronization using a room metaphor",
  "main": "watcher.js",
  "scripts": {
    "test": "cls && mocha",
    "testuser": "cls && mocha --grep user",
    "mongod": "C:\\Programs\\MongoDB\\Server\\4.0\\bin\\mongod.exe -f C:\\Programs\\MongoDB\\Server\\4.0\\bin\\mongod.cfg",
    "help": "cls && node approom.js --help",
    "runserver": "cls && node approom.js runserver",
    "dbsetup": "cls && node approom.js dbsetup",
    "UNUSED_configtest": "cross-env fun=cli node approom.js runserver",
    "UNUSED_configtest2": "cross-env config=debug,dev node approom.js runserver",
    "eslint": "eslint .",
    "npmcheck": "cls && npm audit || npm-check || exit 0",
    "makedoc": "node_modules/.bin/jsdoc --private --readme ../README.md --configure jsdoc.json && copyfiles -u 3 ../docs/jsdoc/**/* static/jsdoc",
    "copydoc": "copyfiles -u 3 --verbose ../docs/jsdoc/**/* static/jsdoc"
  },
