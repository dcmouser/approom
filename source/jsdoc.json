{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "./",
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
		"template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "../docs/jsdoc",
        "recurse": true,
        "verbose": false
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
